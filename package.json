    "start": "node node_modules/react-native/local-cli/cli.js start",
    "android-linux": "python app/config/build/build_id_writer.py && react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && react-native run-android",
    "test": "jest",
    "postinstallios": "cd node_modules/react-native/React/..; exec ./scripts/ios-install-third-party.sh; patch-package",
    "postinstall": "patch-package; cp -Rf ../react-native-gesture-handler node_modules/@react-navigation/native/node_modules",
    "android-release": "python app/config/build/build_id_writer.py && cd android && ./gradlew assembleRelease && adb install -r app/build/outputs/apk/release/app-release.apk",
    "web": "webpack-dev-server -d --config ./web/webpack.dev.js --inline --colors",
    "web-release": "webpack -p --config ./web/webpack.prod.js --progress --mode production"
  },
